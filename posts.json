---
layout: null
---
{
    "posts": [
        {% for post in page.posts %}
        {
            "title": "{{ post.title | xml_escape }}",
            "url": "{{ site.url }}{{ post.url }}",
            "date": "{{ post.date | date_to_xmlschema }}",
            "content": "{{ post.content | markdownify }}"
        }{% unless forloop.last %},{% endunless %}
        {% endfor %}
    ]
}